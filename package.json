{
  "name": "elm-monocle",
  "version": "1.1.0",
  "description": "Library providing purely functional abstractions to manipulate records",
  "scripts": {
    "compile": "elm-make **/*.elm --output monocle.js",
    "compile-and-watch": "echo 'Compiling elm-monocle ...' && npm run compile && onchange '**/*.elm' -v -- npm run compile",
    "test": "elm-test test/TestRunner.elm",
    "test-and-watch": "onchange '**/*.elm' -v -- npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arturopala/elm-monocle.git"
  },
  "keywords": [
    "monocle",
    "lens",
    "iso",
    "prism",
    "optional",
    "record"
  ],
  "author": "Artur Opala",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/arturopala/elm-monocle/issues"
  },
  "homepage": "https://github.com/arturopala/elm-monocle#readme",
  "devDependencies": {
    "elm": "^0.16.0",
    "elm-test": "^0.16.0",
    "onchange": "^2.0.0",
    "ws": "^1.0.1"
  }
}
